    speakerNumber: 0
  - speakerSprite: 2
    speaker: {fileID: 11400000, guid: c326dc43f816e314fb2806cfd46c1e6d, type: 2}
    pos: 1
    sentence: What made you destroy the tree barrier, to begin with?
